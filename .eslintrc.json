{
    "ecmaFeatures": {
        "jsx": false
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
        "space-before-function-paren": [
            "off"
        ]
    }
}